{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "mmr/cblock",
	"version": "0.1.21",
	"title": "Cblock",
	"category": "design",
	"icon": "tablet",
	"description": "MMR Conditional Block",
	"supports": {
		"html": false,
        "spacing": {
            "margin": true, 
            "padding": true,
            "blockGap": true
        }
	},
	"textdomain": "cblock",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
    "attributes": {
        "blockname": {
            "type": "string",
            "default": "conditional block",
            "source": "attribute",
            "selector": "div"
        },
        "description": {
            "type": "string",
            "default": ""
        }
    }
}
